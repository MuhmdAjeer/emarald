	<div id="notfound">
		<div class="notfound">
			<div class="notfound-404"></div>
			{{#if err.status}}
			<h1>{{err.status}}</h1>
			{{else}}
			<h1>404</h1>
			{{/if}}
			<h2>Oops! Page Not Be Found</h2>
			<p>Sorry but the page you are looking for does not exist, have been removed. name changed or is temporarily unavailable</p>
			{{#if user}}
			<a style="color: rgb(219, 106, 0);" class="text-danger" href="/home">Back to homepage</a>
			{{/if }}
			{{#if admin}}
			<a class="text-primary" href="/admin">Back to homepage</a>
			{{/if}}	
			{{#if vendor}}
			<a class="text-primary" href="/vendor/dashboard">Back to homepage</a>
			{{/if }}
		</div>
	</div>